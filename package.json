{
  "name": "sdui",
  "version": "0.0.1",
  "description": "Server-Driven UI",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsup src/main.ts --tsconfig tsconfig.build.json",
    "codegen:openapi": "ts-node --project ./tsconfig.json -r tsconfig-paths/register ./scripts/codegen-openapi.ts",
    "format": "prettier --write \"src/**/*.ts\" \"scripts/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main"
  },
  "dependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/swagger": "^8.1.0",
    "express": "4.21.2",
    "tsup": "^8.4.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
